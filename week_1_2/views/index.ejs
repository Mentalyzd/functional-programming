<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <title>Functional Programming</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
  </head>
  <body>
    <main>
        <h1>Functional Programming</h1>

        <% for(var j=0; j<uniqQuest.length; j++) { %>
            <h3><%= uniqQuest[j] %></h3>
            <h4>Alle antwoorden:</h4>
            <p><%= sortedAnswers[uniqQuest[j]] %></p>
            <h4>Hoevaak is 1 antwoord gegeven:</h4>
            <% for(var i=0; i<Object.entries(countedAnswers[uniqQuest[j]]).length; i++) { %>
                <p><%= Object.entries(countedAnswers[uniqQuest[j]])[i].reverse().toString().replace(",", " antwoorde: ") %></p>
            <% } %>
            <br>
            <br>
        <% } %>

    </main>
  </body>
</html>